# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
# Use either NEXT_PUBLIC_SUPABASE_ANON_KEY or NEXT_PUBLIC_SUPABASE_API (both supported)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
# NEXT_PUBLIC_SUPABASE_API=your-anon-key  # Alternative name
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
# SUPABASE_SECRET_API=your-service-role-key  # Alternative name

# Cloudflare R2 (Document Storage)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
CLOUDFLARE_R2_ACCESS_KEY_ID=your-r2-access-key
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your-r2-secret-key
CLOUDFLARE_R2_BUCKET_NAME=insuredin-documents

# Anthropic (Claude API for AI extraction)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
